{
  "name": "large-reader",
  "version": "0.0.15-alpha",
  "description": "Generate a website for an NFT collection built with Large.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LargeNFT/large-reader/"
  },
  "scripts": {
    "serve": "node ./node_modules/http-server/bin/http-server -p 8081 -c-1",
    "generate": "node node_modules/large-nft/public/reader/generate.js",
    "generate:dev": "node node_modules/large-nft/public/reader/generate.js --env dev",
    "sync": "node --no-experimental-fetch node_modules/large-nft/public/reader/sync.js"

  },
  "keywords": [],
  "author": "Patrick Toner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LargeNFT/large-reader/issues"
  },
  "type": "module",
  "devDependencies": {

    "http-server": "14.1.1",
    "convert-svg-to-png": "0.6.4",

    "large-nft": "*"
  }
}
